msgstr "Escolha o dispositivo Bluetooth"

#: ../src/chooser.gob:151
msgid "Device"
msgstr "Dispositivo"

#: ../src/fileactiondialog.gob:132
msgid "Bluetooth Transfer Received"
msgstr "Transferência via Bluetooth Recebida"

#. FIXME: dname needs escaping
#: ../src/fileactiondialog.gob:138
#, c-format
msgid "File received from '%s'. What would you like to do?"
msgstr "Arquivo recebido de '%s'. O que você gostaria de fazer?"

