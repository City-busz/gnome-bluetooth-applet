#: ../src/chooser.gob:109
msgid "Choose Bluetooth device"
msgstr "ਬਲਿਊਟੁੱਥ ਜੰਤਰ ਚੁਣੋ"

#: ../src/chooser.gob:151
msgid "Device"
msgstr "ਜੰਤਰ"

#: ../src/fileactiondialog.gob:132
