#: ../obex/gnome-obex-server.desktop.in.h:2
msgid "Receive files sent by Bluetooth devices"
msgstr "Ta emot filer som skickas av blåtandsenheter"

#: ../src/chooser.gob:109
msgid "Choose Bluetooth device"
msgstr "Välj blåtandsenhet"

#: ../src/chooser.gob:151
msgid "Device"
msgstr "Enhet"

#: ../src/fileactiondialog.gob:132
msgid "Bluetooth Transfer Received"
msgstr "Blåtandsöverföring mottagen"

#. FIXME: dname needs escaping
#: ../src/fileactiondialog.gob:138
#, c-format
msgid "File received from '%s'. What would you like to do?"
msgstr "Fil mottagen från \"%s\". Vad vill du göra?"

#: ../src/fileactiondialog.gob:168
#, c-format
msgid ""
"<span weight='bold'>Name:</span> %s\n"
"<span weight='bold'>Type:</span> %s\n"
"<span weight='bold'>Size:</span> %s"
msgstr ""
"<span weight='bold'>Namn:</span> %s\n"
"<span weight='bold'>Typ:</span> %s\n"
"<span weight='bold'>Storlek:</span> %s"

#: ../src/permissiondialog.gob:105
#, c-format
msgid "Allow Bluetooth device '%s' to connect?"
msgstr "Tillåt blåtandsenheten \"%s\" att ansluta?"

#: ../src/permissiondialog.gob:121
msgid "An external device is attempting to connect to your computer via Bluetooth. You must decide if you want to accept the connection."
msgstr "En extern enhet försöker att ansluta till din dator via Bluetooth. Du måste välja om du vill acceptera anslutningen."

#: ../src/permissiondialog.gob:127
msgid "_Always allow this device to connect."
msgstr "_Tillåt alltid denna enhet att ansluta."

#: ../ui/gnome-obex-send.glade.h:1
msgid "<span weight=\"bold\" size=\"larger\">Sending file</span>"
msgstr "<span weight=\"bold\" size=\"larger\">Skickar fil</span>"

#: ../ui/gnome-obex-send.glade.h:2
msgid "Bluetooth file transfer"
msgstr "Filöverföring via Bluetooth"

