#: ../data/gnome-obex-server.schemas.in.h:1
msgid "Directory to save received files"
msgstr "Katalog för att spara mottagna filer"

#: ../data/gnome-obex-server.schemas.in.h:2
msgid "Display dialog when file received"
msgstr "Visa dialogfönster då en fil tas emot"

#: ../data/gnome-obex-server.schemas.in.h:3
msgid "Incoming files are saved automatically into this directory. If display-notification is set then the user gets chance to open or delete them."
msgstr "Inkommande filer sparas automatiskt i denna katalog. Om displaynotifiering är aktiverat får användaren en möjlighet att öppna eller ta bort dem."

#: ../data/gnome-obex-server.schemas.in.h:4
msgid "Received files are automatically saved. If this option is set, then the user is prompted with a dialog which allows them to view or delete the received file."
msgstr "Mottagna filer sparas automatiskt. Om detta alternativ är aktiverat kommer användaren att få ett dialogfönster som låter dem visa eller ta bort den mottagna filen."

#: ../obex/gnome-obex-send.c:193
#, c-format
msgid "Unable to read file '%s'."
msgstr "Kan inte läsa filen \"%s\"."

#: ../obex/gnome-obex-send.c:224
msgid "An error occurred while sending data."
msgstr "Ett fel inträffade vid skickande av data."

#: ../obex/gnome-obex-send.c:227
msgid "The remote device canceled the transfer."
msgstr "Fjärrenheten avbröt överföringen."

#: ../obex/gnome-obex-send.c:230
msgid "An unknown error occurred."
msgstr "Ett oväntat fel inträffade."

#: ../obex/gnome-obex-send.c:263
msgid "Connecting..."
msgstr "Ansluter..."

#: ../obex/gnome-obex-send.c:316
msgid "Can't find user interface file, check your gnome-bluetooth installation."
msgstr "Kan inte hitta användargränssnittsfil. Kontrollera din installation av gnome-bluetooth."

#: ../obex/gnome-obex-send.c:332
msgid "Make sure \"Beam receive\" is enabled in the Power preferences, and Bluetooth is enabled."
msgstr "Se till att \"Beam receive\" är aktiverad i ströminställningarna och att Bluetooth är aktiverat."

#: ../obex/gnome-obex-send.c:334
msgid "Remote device doesn't support receiving objects."
msgstr "Fjärrenheten stöder inte mottagande objekt."

#: ../obex/gnome-obex-send.c:344
msgid "Can't initialize program."
msgstr "Kan inte initiera program."

#: ../obex/gnome-obex-send.c:350
msgid "Unable to connect to remote device."
msgstr "Kan inte ansluta till fjärrenheten."

#: ../obex/gnome-obex-server.c:161
#, c-format
msgid "Accept a file from '%s'?"
msgstr "Acceptera en fil från \"%s\"?"

#: ../obex/gnome-obex-server.c:162
msgid "The remote device is attempting to send you a file via Bluetooth. Do you wish to allow the connection?"
msgstr "Fjärrenheten försöker att skicka en fil till dig via Bluetooth. Vill du tillåta anslutningen?"

#: ../obex/gnome-obex-server.c:163
msgid "Always accept files from this device."
msgstr "Acceptera alltid filer från denna enhet."

#. The name of the default downloads folder,
#. * Needs to be the same as Epiphany's
#: ../obex/gnome-obex-server.c:204
msgid "Downloads"
msgstr "Hämtningar"

#: ../obex/gnome-obex-server.c:295
msgid "Incoming Bluetooth file transfer"
msgstr "Inkommande filöverföring via Bluetooth"

#: ../obex/gnome-obex-server.c:337
#: ../obex/gnome-obex-server.c:504
msgid "Ready for Bluetooth file transfers"
msgstr "Redo för filöverföringar via Bluetooth"

