#: ../obex/gnome-obex-server.c:389
#: ../obex/gnome-obex-server.desktop.in.h:1
msgid "Bluetooth File Sharing"
msgstr "Fildelning via Bluetooth"

#: ../obex/gnome-obex-server.c:392
msgid "Receive files from Bluetooth devices"
msgstr "Mottag filer från blåtandsenheter"

